<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   <bean id = "order" class = "com.oocl.barceja2.hw2.ordersClass" >
	<constructor-arg index="0" value="Soap" />
	<constructor-arg index="1" value="2001" />
	<constructor-arg index="2" value="Suhay" />
	</bean>
 
 	<bean id = "classA" class = "com.oocl.barceja2.hw2.classA" >
