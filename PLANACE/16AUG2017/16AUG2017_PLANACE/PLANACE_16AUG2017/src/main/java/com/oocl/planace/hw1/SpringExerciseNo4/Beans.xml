<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="getOrder" class="com.oocl.planace.hw1.SpringExerciseNo4.Payment">
		<property name="order">
			<bean id="orderObj" class="com.oocl.planace.hw1.SpringExerciseNo4.Order">
				<property name="item" value="Ice Cream" />
				<property name="price" value="300" />
				<property name="qty" value="3" />
			</bean>
		</property>
	</bean>


</beans>