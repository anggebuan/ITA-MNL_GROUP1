<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="company" class="com.oocl.planace.hw3.SpringExerciseNo6.Company">
		<property name="companyName" value="OOCL" />
		<property name="companyAddress" value="HKG" />
		<property name="companyNo" value="1234567" />
	</bean>

	<bean id="employee" parent="company">
		<property name="employeeName" value="Nonie" />
	</bean>
</beans>