<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- Spring Exercise 3 -->
	<bean id="drawObjCircle" class="com.oocl.planace.sw1.SpringExercise.Drawing">
		<property name="shape">
			<bean id="circleObj" class="com.oocl.planace.sw1.SpringExercise.Circle" />
		</property>
	</bean>

	<bean id="drawObjTriangle" class="com.oocl.planace.sw1.SpringExercise.Drawing">
		<property name="shape">
			<bean id="triangleObj" class="com.oocl.planace.sw1.SpringExercise.Triangle" />
		</property>
	</bean>

</beans>